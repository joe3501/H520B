    332          /**
    333          * @brief	维护主状态机的线程
    334          */
    335          void State_Machine_thread(void *p)
    336          {
    337          	unsigned int	i,cnt,event;
    338          	unsigned char	err;
    339          	unsigned char	barcode[MAX_BARCODE_LEN+1];
    340          	unsigned char   codetype[20];
    341          	unsigned int    codelen;
    342          	int				ret,index;
    343          	unsigned int	last_state;
    344          	unsigned char	*rec;
    345          
    346          	Jfree(p_init_thread_stk);	//退出初始化线程时，释放自己的任务栈
    347          
    348          	//hw_platform_led_blink_test();		//for test
    349          	//lowpower_tip();					//for test
    350          	//record_m_test();					//for test
    351          	
    352          	while(1)
    353          	{
    354          		event = (unsigned int)OSQPend(pEvent_Queue,25,&err);
    355          		if (event == 0)
    356          		{
    357          			if ((g_param.lower_power_timeout)&&(device_current_state != STATE_HID_Mode))
    358          			{
    359          				keypress_timeout++;
    360          				if (keypress_timeout == g_param.lower_power_timeout*4*60)
    361          				{
    362          					hw_platform_beep_ctrl(500,3000);
    363          #ifndef DEBUG_VER
    364          					EnterLowPowerMode();
    365          					ExitLowPowerMode();
    366          #endif	
    367          					hw_platform_beep_ctrl(500,3000);
    368          				}
    369          			}
    370          			continue;
    371          		}
    372          #ifdef DEBUG_VER
    373          		printf("current state:%d\r\n",device_current_state);
    374          		printf("event:%d\r\n",event);
    375          #endif
    376          		if(device_current_state ==  STATE_BT_Mode_Disconnect)
    377          		{
    378          			switch(event)
    379          			{
    380          			case EVENT_SCAN_KEY_SINGLE_CLICK:
    381          			case EVENT_SCAN_KEY_DOUBLE_CLICK:
    382          				ret = scanner_get_barcode(barcode,MAX_BARCODE_LEN,codetype,&codelen);	//扫描条码
    383          				hw_platform_stop_led_blink(LED_GREEN);
    384          				if (ret == 0)
    385          				{
    386          					scan_barcode_ok_tip();
    387          				}
    388          
    389          				if (lowpower_state)
    390          				{
    391          					lowpower_tip();
    392          				}
    393          				//只是扫描到条码而已，什么都不做
    394          				break;
    395          			case EVENT_SCAN_KEY_LONG_PRESS:
    396          				//切换到Memory Mode
    397          				hw_platform_stop_led_blink(LED_GREEN);
    398          				exit_from_BT_Mode(0);
    399          				device_current_state = STATE_Memory_Mode;
    400          				hw_platform_beep_ctrl(300,3000);
    401          				enter_into_Memory_Mode();
    402          				break;
    403          			case EVENT_ERASE_KEY_SINGLE_CLICK:
    404          				break;
    405          			case EVENT_ERASE_KEY_LONG_PRESS:
    406          				//切换到配对模式
    407          				exit_from_BT_Mode(0);
    408          				device_current_state = STATE_BT_Mode_WaitPair;
    409          				enter_into_BT_Mode(2);
    410          				break;
    411          			case EVENT_RESET_KEY_PRESS:
    412          				break;
    413          			case EVENT_BT_CONNECTED:
    414          				//切换到蓝牙连接模式
    415          				exit_from_BT_Mode(0);
    416          				device_current_state = STATE_BT_Mode_Connect;
    417          				enter_into_BT_Mode(1);
    418          				break;
    419          			case EVENT_BT_DISCONNECTED:
    420          				break;
    421          			case EVENT_USB_CABLE_INSERT:
    422          				//切换到USB HID模式
    423          				exit_from_BT_Mode(0);
    424          				last_state = STATE_BT_Mode_Disconnect;
    425          				device_current_state = STATE_HID_Mode;
    426          				enter_into_USB_HID_Mode();
    427          				break;
    428          			case EVENT_USB_CABLE_REMOVE:
    429          				break;
    430          			case EVENT_LOW_POWER:
    431          				lowpower_tip();
    432          				break;
    433          			default:
    434          				break;
    435          			}
    436          		}
    437          		else if(device_current_state ==  STATE_BT_Mode_Connect)
    438          		{
    439          			switch(event)
    440          			{
    441          			case EVENT_SCAN_KEY_SINGLE_CLICK:
    442          				//扫描条码
    443          				ret = scanner_get_barcode(barcode,MAX_BARCODE_LEN,codetype,&codelen);
    444          				hw_platform_stop_led_blink(LED_GREEN);
    445          				if (ret != 0)
    446          				{
    447          					break;
    448          				}
    449          
    450          				scan_barcode_ok_tip();
    451          				if (lowpower_state)
    452          				{
    453          					lowpower_tip();
    454          				}
    455          				//扫描到条码了
    456          				//将获取到的条码先push到cash缓存起来，然后Post到系统的
    457          				//Queue，由蓝牙模块线程负责去发送到主机
    458          repost:
    459          				ret = OSQPost(pBarcode_Queue,(void*)push_barcode_into_cash((unsigned char*)barcode));
    460          				if(ret != OS_ERR_NONE)
    461          				{
    462          					if(ret == OS_ERR_Q_FULL || ret == OS_ERR_PEVENT_NULL)
    463          					{
    464          						//如果队列满了或者空事件时，那么需要延时重试
    465          						OSTimeDlyHMSM(0,0,0,100);
    466          						goto repost;
    467          					}
    468          					else
    469          					{
    470          						assert(0);	//系统错误
    471          					}
    472          				}
    473          				break;
    474          			case EVENT_SCAN_KEY_DOUBLE_CLICK:
    475          				if (g_param.ios_softkeypad_enable)
    476          				{
    477          #if(BT_MODULE == USE_WBTDS01)
    478          					WBTD_set_ioskeypad(1);
    479          #else
    480          					BT816_toggle_ioskeypad();
    481          #endif
    482          				}
    483          				break;
    484          			case EVENT_SCAN_KEY_LONG_PRESS:
