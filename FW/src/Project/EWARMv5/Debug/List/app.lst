    878          		if ((ret == BT_MODULE_STATUS_CONNECTED) || (ret == BT_MODULE_STATUS_DISCONNECT))
    879          		{
    880          #ifdef DEBUG_VER
    881          			printf("BT Module Status = %s!\r\n",(ret==1)?"Connected":"Disconnect");
    882          #endif
    883          			OSQPost(pEvent_Queue,(void*)((ret == BT_MODULE_STATUS_CONNECTED)?EVENT_BT_CONNECTED:EVENT_BT_DISCONNECTED));
    884          		}
    885          #else
    886          		ret = BT816_hid_status();
    887          		if ((ret == BT_MODULE_STATUS_CONNECTED)||(ret == BT_MODULE_STATUS_DISCONNECT))
    888          		{
    889          			if (ret != last_status)
    890          			{
    891          				last_status = ret;
    892          
    893          #ifdef DEBUG_VER
    894          				printf("BT Module Status = %s!\r\n",(ret==BT_MODULE_STATUS_CONNECTED)?"Connected":"Disconnect");
    895          #endif
    896          				OSQPost(pEvent_Queue,(void*)((ret == BT_MODULE_STATUS_CONNECTED)?EVENT_BT_CONNECTED:EVENT_BT_DISCONNECTED));
    897          			}
    898          		}
    899          #endif
    900          
    901          
    902          		pbarcode = (unsigned char*)OSQPend(pBarcode_Queue,20,&err);
    903          		if (pbarcode)
    904          		{
    905          #ifdef DEBUG_VER
    906          			printf("BT Module got data(%s) to send!\r\n",pbarcode);
    907          #endif
    908          #if(BT_MODULE == USE_WBTDS01)
    909          			if (WBTD_hid_send(pbarcode,strlen((char const*)pbarcode),&len))
    910          #else
    911          			if (BT816_hid_send(pbarcode,strlen((char const*)pbarcode)))
    912          #endif
    913          			{
    914          				//发送失败应该怎么处理，什么都不做了么????!!!!
    915          				//@todo...
    916          #ifdef DEBUG_VER
    917          				printf("BT Module send data Fail!\r\n");
    918          #endif
    919          			}
    920          			else
    921          			{
    922          				pull_barcode_from_cash(pbarcode);
    923          				//OSTimeDlyHMSM(0,0,0,50);
    924          
    925          #ifdef DEBUG_VER
    926          				printf("BT Module send data Success!\r\n");
    927          #endif
    928          			}
