    760          /**
    761          * @brief	获取部分异步事件的线程
    762          * @note     另外一些异步事件的检测在中断服务程序中post或者另外的线程post出来
    763          *			比如：按键事件会在定时器中断的服务程序post出来
    764          *				  USB线的插入（实际上是USB HID device被枚举成功）会在USB的中断服务程序给出
    765          *				  蓝牙连接的状态变化会在蓝牙模块的维护线程给出
    766          *				  此线程只负责一些周期性的状态检查事件，电池电量低、USB线是否被拔出
    767          */
    768          void Event_capture_thread(void *p)
    769          {
    770          #ifdef DEBUG_VER
    771          	printf("Enter into Event_capture_thread!\r\n");
    772          #endif
    773          	while (1)
    774          	{
    775          		if (device_current_state == STATE_HID_Mode)
    776          		{
    777          			lowpower_state = 0;
    778          			lowpower_cnt = 0;
    779          			//判断USB线的拔出
    780          			if (bDeviceState == UNCONNECTED)
    781          			{
    782          #ifdef DEBUG_VER
    783          				printf("usb cable remove detected!\r\n");
    784          #endif
    785          				OSQPost(pEvent_Queue,(void*)EVENT_USB_CABLE_REMOVE);
    786          			}
    787          
    788          			if (hw_platform_ChargeState_Detect())
    789          			{
    790          				//充电完成
    791          				hw_platform_led_ctrl(LED_RED,0);
    792          			}
    793          
    794          			OSTimeDlyHMSM(0,0,0,50);	//50ms的频率运行此线程
    795          		}
    796          		else
    797          		{
    798          			//判断电池电量低
    799          			if (hw_platform_get_PowerClass() == 0)
    800          			{
    801          				lowpower_cnt++;
    802          				if (lowpower_cnt>10)
    803          				{
    804          #ifdef DEBUG_VER
    805          					printf("low power detected!\r\n");
    806          #endif
    807          					if (lowpower_state == 0)
    808          					{
    809          						OSQPost(pEvent_Queue,(void*)EVENT_LOW_POWER);
    810          						lowpower_state = 1;
    811          					}
    812          					
    813          				}
    814          			}
    815          			else
    816          			{
    817          				lowpower_cnt = 0;
    818          			}
    819          
    820          			if (bDeviceState == CONFIGURED)
    821          			{
    822          #ifdef DEBUG_VER
    823          				printf("USB HID Enum OK detected!\r\n");
    824          #endif
    825          				OSQPost(pEvent_Queue,(void*)EVENT_USB_CABLE_INSERT);
    826          			}
    827          			OSTimeDlyHMSM(0,0,0,50);	//50ms的频率运行此线程
    828          		}
    829          	}
    830          }
